{
	"compilerOptions": {
		"declaration": true,
		"emitDecoratorMetadata": false,
		"experimentalDecorators": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"noFallthroughCasesInSwitch": false,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"removeComments": false,
		"sourceMap": false,
		"strictNullChecks": false,
		"target": "es5",
		"allowJs": false,
		"outDir": "dist"
	}
}